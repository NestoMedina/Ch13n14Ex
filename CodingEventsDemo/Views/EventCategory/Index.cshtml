@model List<EventCategory>

<h1>Event Categories</h1>

@if (Model.Count == 0)
{
    <p>No event categories yet!</p>
}

<table>
    @foreach (EventCategory kvp in Model)
    {
        <tr>
            <th>ID</th>
            <th>Event Name</th>
        </tr>
        <tr>
            <td>@kvp.Id</td>
            <td><a href="/Events/EventInfo">@kvp.Name</a></td>
        </tr>
    }

</table>
